{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "result": {
      "type": "array",
      "items": { "type": "object" }
    },
    "page": {
      "type": "object",
      "properties": {
        "total": { "type": "integer" },
        "totalPages": { "type": "integer" },
        "pageNumber": { "type": "integer" },
        "pageSize": { "type": "integer" },
        "next": { "type": "string" },
        "previous": { "type": "string" }
      },
      "required": ["total", "totalPages", "pageNumber", "pageSize", "next", "previous"]
    }
  },
  "required": ["result", "page"]
}
