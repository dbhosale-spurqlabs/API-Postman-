{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["result"],
  "properties": {
    "result": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "state", "borrowerName", "borrowerCountry", "sellOfferSize", "currency"],
        "properties": {
          "id": {
            "type": "string"
          },
          "state": {
            "type": "string",
            "enum": ["CLOSED"]
          },
          "borrowerName": {
            "type": "string"
          },
          "borrowerCountry": {
            "type": "string"
          },
          "sellOfferSize": {
            "type": ["number", "string"]
          },
          "currency": {
            "type": "string"
          }
        }
      }
    }
  }
}